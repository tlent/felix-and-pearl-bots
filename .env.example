# Discord webhook URL for both bots
DISCORD_WEBHOOK_URL=your_discord_webhook_url

# Anthropic API key for Claude
ANTHROPIC_API_KEY=your_anthropic_api_key

# OpenWeather API key
WEATHER_API_KEY=your_weather_api_key

# Weather location
WEATHER_LOCATION=City,State,Country

# Weather coordinates (required for OpenWeather One Call API 3.0)
WEATHER_LAT=40.7128
WEATHER_LON=-74.0060

# AWS region for Parameter Store
AWS_DEFAULT_REGION=us-east-1

# Birthday configuration (JSON string)
BIRTHDAYS_CONFIG={"MM-DD": {"name": "display_name"}} 